    overflow-y: auto;
    background-color: var(--card-bg);
    border: 1px solid var(--border-color);
    border-radius: 4px;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    z-index: 101; /* Higher than container */
    margin-top: 4px;
  }
  
  .poam-dropdown-search {
    width: calc(100% - 16px);
    margin: 8px;
    padding: 6px 8px;
    border: 1px solid var(--border-color);
    border-radius: 4px;
  }
  
  .poam-dropdown-items {
    padding: 0 8px 8px;
  }
  
  .poam-dropdown-item {
    display: flex;
    align-items: center;
    padding: 6px 8px;
    cursor: pointer;
    border-radius: 4px;
  }
  
  .poam-dropdown-item:hover {
    background-color: var(--hover-bg);
  }
  
  .form-group-editor.expanded {
    flex-grow: 1;
    display: flex;
    flex-direction: column;
    margin-bottom: 0;
  }
  
  .form-group-editor.expanded .note-rich-text-editor {
    flex-grow: 1;
    min-height: 250px;
    display: flex;
    flex-direction: column;
  }
  
  .form-group-editor.expanded .ql-container {
    flex-grow: 1;
    max-height: calc(100vh - 300px);
    overflow-y: auto;
  }
  
  .icon-btn.small {
    padding: 2px;
    font-size: 14px;
  }
  
  /* Modern Note Cards - Obsidian/Notion Style */
  .notes-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));
    gap: 20px;
    padding: 16px;
    width: 100%;
  }
  
  .note-card {
    position: relative;
    border-radius: 8px;
    background-color: var(--card-bg);
    border: 1px solid var(--border-color);
    overflow: hidden;
    transition: all 0.2s ease;
    cursor: pointer;
    height: 100%;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.05);
  }
  
  .note-card:hover {
    transform: translateY(-2px);
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);
    border-color: var(--primary-color);
  }
  
  .note-card-inner {
    display: flex;
    flex-direction: column;
    height: 100%;
    padding: 16px;
  }
  
  .note-card-header {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    margin-bottom: 8px;
  }
  
  .note-card-title {
    font-size: 1.1rem;
    font-weight: 600;
    margin: 0;
    color: var(--text-color);
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height: 1.4;
    flex: 1;
  }
  
  .note-card-actions {
    display: flex;
    gap: 4px;
    opacity: 0;
    transition: opacity 0.2s ease;
  }
  
  .note-card:hover .note-card-actions {
    opacity: 1;
  }
  
  .note-action-btn {
    padding: 4px !important;
    height: 28px;
    width: 28px;
    border-radius: 4px;
    background-color: transparent !important;
    border: none !important;
    box-shadow: none !important;
    color: var(--text-secondary) !important;
    transition: all 0.2s ease;
  }
  
  .note-action-btn:hover {
    background-color: var(--hover-bg) !important;
    color: var(--text-color) !important;
  }
  
  .note-action-danger:hover {
    color: var(--danger-color) !important;
  }
  
  .note-card-meta {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    gap: 8px;
    margin-bottom: 12px;
    flex-wrap: wrap;
  }
  
  .note-card-date {
    font-size: 0.8rem;
    color: var(--text-secondary);
    flex-shrink: 0;
  }
  
  .note-card-tags {
    display: flex;
    flex-wrap: wrap;
    gap: 4px;
  }
  
  .note-card-tag {
    display: inline-block;
    font-size: 0.7rem;
    border-radius: 4px;
    padding: 2px 6px;
    background-color: var(--primary-color);
    color: white;
    font-weight: 500;
    opacity: 0.8;
  }
  
  .note-card-tag-more {
    background-color: var(--gray-color);
  }
  
  .note-card-content {
    flex: 1;
    overflow: hidden;
    font-size: 0.9rem;
    line-height: 1.5;
    color: var(--text-secondary);
  }
  
  .note-card-content img {
    max-width: 100%;
    height: auto;
  }
  
  .note-card-content p {
    margin-top: 0;
    margin-bottom: 0.5em;
  }
  
  .note-card-content h1, 
  .note-card-content h2, 
  .note-card-content h3, 
  .note-card-content h4,
  .note-card-content h5,
  .note-card-content h6 {
    margin-top: 0.5em;
    margin-bottom: 0.5em;
  }
  
  .note-card-content ul, 
  .note-card-content ol {
    padding-left: 1.5em;
  }
  
  .notes-empty-state {
    padding: 40px;
    text-align: center;
    color: var(--text-secondary);
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    min-height: 200px;
    border: 1px dashed var(--border-color);
    border-radius: 8px;
    margin: 16px;
  }
  
  .notes-empty-state p {
    font-size: 1rem;
    opacity: 0.8;
  }
  
  /* Ensure the card preview cuts off properly */
  .note-card-content::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 40px;
    background: linear-gradient(transparent, var(--card-bg));
    pointer-events: none;
  }
  
  @media (max-width: 768px) {
    .notes-grid {
      grid-template-columns: 1fr;
      gap: 16px;
      padding: 12px;
    }
  }
  
  /* Notion-inspired Note View styles */
  .notion-page {
    width: 100%;
    max-width: 100%;
    padding: 0 8px;
    margin: 0 auto;
    background-color: var(--card-bg);
    min-height: 100%;
    display: flex;
    flex-direction: column;
    color: var(--text-color);
    font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Helvetica, "Apple Color Emoji", Arial, sans-serif, "Segoe UI Emoji", "Segoe UI Symbol";
  }
  
  .notion-header {
    width: 100%;
    max-width: 900px;
    margin: 0 auto;
    padding: 24px 0 0;
  }
  
  .notion-title {
    font-size: 2.5rem;
    font-weight: 700;
    margin: 0;
    padding: 0;
    line-height: 1.3;
    letter-spacing: -0.01em;
    color: var(--text-color);
    overflow-wrap: break-word;
    outline: none;
  }
  
  .notion-title:empty::before {
    content: "Untitled";
    color: var(--text-secondary);
    opacity: 0.6;
  }
  
  .notion-metadata {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    gap: 16px;
    margin-top: 12px;
    color: var(--text-secondary);
    font-size: 14px;
  }
  
  .notion-date {
    color: var(--text-secondary);
    font-size: 0.9rem;
  }
  
  .notion-tags {
    display: flex;
    flex-wrap: wrap;
    gap: 6px;
  }
  
  .notion-tag {
    font-size: 0.85rem;
    line-height: 1.2;
    color: rgb(50, 48, 44);
    background: rgba(227, 226, 224, 0.5);
    border-radius: 3px;
    padding: 4px 8px;
    font-weight: 500;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    max-width: 200px;
  }
  
  .dark-theme .notion-tag {
    background: rgba(255, 255, 255, 0.08);
    color: rgba(255, 255, 255, 0.9);
  }
  
  .notion-divider {
    width: 100%;
    max-width: 900px;
    margin: 16px auto;
    height: 1px;
    background-color: var(--border-color);
  }
  
  .notion-content {
    width: 100%;
    max-width: 900px;
    margin: 0 auto;
    padding: 8px 0 120px;
    flex: 1;
  }
  
  .notion-text-content {
    display: flex;
    flex-direction: column;
    width: 100%;
    line-height: 1.5;
    font-size: 1rem;
    color: var(--text-color);
  }
  
  .notion-text-content * {
    max-width: 100%;
    word-wrap: break-word;
    overflow-wrap: break-word;
    white-space: pre-wrap;
  }
  
  .notion-text-content h1,
  .notion-text-content h2,
  .notion-text-content h3,
  .notion-text-content h4,
  .notion-text-content h5,
  .notion-text-content h6 {
    margin-top: 1.4em;
    margin-bottom: 0.5em;
    font-weight: 600;
    line-height: 1.3;
    letter-spacing: -0.01em;
    color: var(--text-color);
  }
  
  .notion-text-content h1 {
    font-size: 1.875rem;
    margin-top: 2rem;
  }
  
  .notion-text-content h2 {
    font-size: 1.5rem;
    margin-top: 1.4rem;
  }
  
  .notion-text-content h3 {
    font-size: 1.25rem;
    margin-top: 1rem;
  }
  
  .notion-text-content p {
    margin: 1px 0;
    padding: 3px 2px;
    min-height: 1.5em;
    white-space: pre-wrap;
  }
  
  .notion-text-content ul,
  .notion-text-content ol {
    margin: 0.6em 0;
    padding-left: 1.4em;
  }
  
  .notion-text-content li {
    padding: 2px 0;
    margin: 0;
  }
  
  .notion-text-content li > p {
    margin: 0;
  }
  
  .notion-text-content pre {
    background-color: var(--surface-color);
    border-radius: 4px;
    padding: 16px;
    overflow-x: auto;
    font-family: "SFMono-Regular", Consolas, "Liberation Mono", Menlo, monospace;
    font-size: 0.9em;
    line-height: 1.5;
    margin: 0.7em 0;
    tab-size: 2;
  }
  
  .notion-text-content code {
    background-color: rgba(135, 131, 120, 0.15);
    color: #eb5757;
    padding: 0.2em 0.4em;
    border-radius: 3px;
    font-family: "SFMono-Regular", Consolas, "Liberation Mono", Menlo, monospace;
    font-size: 0.85em;
    white-space: pre-wrap;
  }
  
  .dark-theme .notion-text-content code {
    background-color: rgba(135, 131, 120, 0.15);
    color: #ff6b6b;
  }
  
  .notion-text-content blockquote {
    margin: 0.7em 0;
    padding: 0.2em 0 0.2em 0.9em;
    border-left: 3px solid rgba(55, 53, 47, 0.16);
    font-style: italic;
    color: var(--text-secondary);
  }
  
  .dark-theme .notion-text-content blockquote {
    border-left-color: rgba(255, 255, 255, 0.16);
  }
  
  .notion-text-content img {
    max-width: 100%;
    height: auto;
    border-radius: 3px;
    margin: 0.7em 0;
  }
  
  .notion-text-content a {
    color: var(--primary-color);
    text-decoration: underline;
    text-underline-offset: 1px;
    transition: opacity 0.2s;
  }
  
  .notion-text-content a:hover {
    opacity: 0.8;
  }
  
  .notion-text-content hr {
    margin: 2em 0;
    border: none;
    border-top: 1px solid var(--border-color);
  }
  
  .notion-text-content table {
    border-collapse: collapse;
    width: 100%;
    margin: 1em 0;
    font-size: 0.9em;
  }
  
  .notion-text-content table th,
  .notion-text-content table td {
    border: 1px solid var(--border-color);
    padding: 0.5em;
  }
  
  .notion-text-content table th {
    background-color: var(--surface-color);
    font-weight: 500;
  }
  
  .notion-text-content .ql-editor-view {
    padding: 0;
  }
  
  /* Fix for the old content style */
  .note-view-content-text {
    margin-top: 1rem;
  }
  
  .note-view-content-text strong {
    display: none; /* Hide the "Content:" label */
  }
  
  .note-view-content-text .ql-editor-view {
    background-color: transparent;
    padding: 0;
    border: none;
    line-height: 1.5;
  }
  
  .note-view-content-text p {
    background-color: transparent;
    padding: 0;
    border: none;
    margin: 0.7em 0;
    white-space: pre-wrap;
    line-height: 1.5;
  }
  
  /* Restore responsive styles */
  @media (max-width: 768px) {
    .notion-title {
      font-size: 1.8rem;
    }
    
    .notion-header, 
    .notion-content, 
    .notion-divider {
      max-width: 100%;
      padding-left: 16px;
      padding-right: 16px;
    }
  }
  
  .dark-theme .notion-text-content blockquote {
    border-left-color: rgba(255, 255, 255, 0.16);
  }
  
  .notion-placeholder {
    color: var(--text-secondary);
    opacity: 0.5;
    font-style: italic;
    margin-top: 1.5rem;
    text-align: center;
    font-size: 0.95rem;
  }
  
  .notion-text-content img {
    max-width: 100%;
    height: auto;
    border-radius: 3px;
    margin: 0.7em 0;
  }
  
  /* Enhance the milestone textarea specifically */
  .milestone-modal textarea {
    min-height: 220px;
    padding: 15px;
    line-height: 1.6;
    font-size: 1rem;
    width: 100%;
    resize: vertical;
    border-radius: 6px;
    transition: border 0.2s, box-shadow 0.2s;
    box-shadow: inset 0 1px 3px rgba(0, 0, 0, 0.05);
  }
  
  .milestone-modal textarea:focus {
    box-shadow: 0 0 0 2px rgba(var(--primary-color-rgb), 0.3), inset 0 1px 3px rgba(0, 0, 0, 0.05);
    border-color: var(--primary-color);
    outline: none;
  }
  
  .milestone-modal .form-group {
    margin-bottom: 20px;
  }
  
  .milestone-modal .modal-content {
    padding: 25px;
  }
  
  /* Animation for modal entrance */
  @keyframes modalFadeIn {
    from {
      opacity: 0;
      transform: translateY(-20px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }
  
  .milestone-modal {
    animation: modalFadeIn 0.3s ease forwards;
  }
  
  .milestone-modal .modal-footer {
    padding: 20px 25px;
    border-top: 1px solid var(--border-color);
    display: flex;
    justify-content: flex-end;
    gap: 15px;
  }
  
  /* Responsive adjustments */
  @media (max-width: 768px) {
    .milestone-modal {
      max-width: 95%;
      max-height: 85vh;
    }
    
    .milestone-modal .form-grid {
      grid-template-columns: 1fr;
      gap: 15px;
    }
    
    .milestone-modal .form-grid .full-width,
    .milestone-modal .form-grid .description-field {
      grid-column: 1;
    }
    
    .milestone-modal textarea {
      min-height: 180px;
    }
    
    .milestone-modal .modal-header,
    .milestone-modal .modal-content,
    .milestone-modal .modal-footer {
      padding: 15px;
    }
  }
  
  /* Add section dividers for visual organization */
  .milestone-modal .form-section {
    margin-bottom: 25px;
  }
  
  .milestone-modal .form-section-title {
    font-size: 1.1rem;
    font-weight: 600;
    margin-bottom: 18px;
    padding-bottom: 8px;
    border-bottom: 1px solid var(--border-color);
    color: var(--text-color);
  }
  
  /* Medium-sized screens optimization */
  @media (min-width: 769px) and (max-width: 1024px) {
    .milestone-modal {
      max-width: 90%;
    }
    
    .milestone-modal .form-grid {
      gap: 15px;
    }
  }